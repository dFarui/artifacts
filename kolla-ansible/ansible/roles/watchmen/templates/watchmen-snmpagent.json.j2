{
    "command": "/usr/bin/watchmen-snmpagent",
    "config_files": [
        {
            "source": "{{ container_config_directory }}/watchmen-snmpagent.conf",
            "dest": "/etc/watchmen/watchmen-snmpagent.conf",
            "owner": "root:{{ watchmen_group_name }}",
            "perm": "0640"
        },
        {
            "source": "{{ container_config_directory }}/watchmen-snmpagent-logging.conf",
            "dest": "/etc/watchmen/watchmen-snmpagent-logging.conf",
            "owner": "root:{{ watchmen_group_name }}",
            "perm": "0640"
        },
        {
            "source": "{{ container_config_directory }}/watchmen-producer.conf",
            "dest": "/etc/watchmen/watchmen-producer.conf",
            "owner": "root:{{ watchmen_group_name }}",
            "perm": "0640"
        },
        {
            "source": "{{ container_config_directory }}/watchmen-extra-whitelist.yml",
            "dest": "/etc/watchmen/watchmen-extra-whitelist.yml",
            "owner": "root:{{ watchmen_group_name }}",
            "perm": "0640"
        }
    ]
}
